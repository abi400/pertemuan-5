#include <iostream>

using namespace std;

int main()
{
    int angka ;

    cout << "masukkan angka; ";
    cin >> angka;

    if(angka==0){
        cout <<"angka Nol";
    }

    else if(angka>0){
        cout <<angka<<" adalah angka positif";
    }
    else {
        cout <<angka<<" adalah angka negatif";
    }

}
